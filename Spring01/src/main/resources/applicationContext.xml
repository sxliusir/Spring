<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

        <bean id="person" name="human" class="com.sxliusir.spring.Person">
            <property name="name"><value>张三</value></property>
            <property name="age" value="18"></property>
            <property name="food" ref="food"></property>
            <property name="list">
                <array>
                    <value>北京</value>
                    <value>上海</value>
                    <value>成都</value>
                </array>
            </property>
            <property name="array">
                <array>
                    <value>a</value>
                    <value>b</value>
                    <value>c</value>
                </array>
            </property>
            <property name="map">
                <props>
                    <prop key="a">1</prop>
                    <prop key="b">2</prop>
                </props>
            </property>
            <property name="skill">
                <value>
                    run=100;
                    power=200
                </value>
            </property>
            <property name="set">
                <array>
                    <value>a</value>
                    <value>b</value>
                    <value>c</value>
                </array>
            </property>
        </bean>

        <bean id="food" class="com.sxliusir.spring.Food"/>

        <bean id="car" class="com.sxliusir.spring.CarFactory" factory-method="getCar">
            <constructor-arg value="audi"/>
        </bean>
</beans>